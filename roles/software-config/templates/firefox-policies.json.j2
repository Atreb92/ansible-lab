{
    "policies": {
      "Bookmarks": [
        {% for item in firefox_bookmarks %}
          {
          "Title": "{{ item.title }}",
          "URL": "{{ item.url }}",
          "Placement": "{{ item.placement }}",
          "Folder": "{{ item.folder }}"
        }{{ "," if not loop.last }}
        {% endfor %}
      ],
      "Certificates": {
        "Install": [
        ]
      },
      "DisableAppUpdate": false,
      "DisplayBookmarksToolbar": true,
      "NoDefaultBookmarks": true,
      "OverrideFirstRunPage": "",
      "OverridePostUpdatePage": "",
      "Homepage": {
        "URL": "file:///usr/share/kali-defaults/web/homepage.html",
        "Locked": false,
        "StartPage": "homepage"
      },
      "SearchEngines": {
        "Default": "Google",
        "PreventInstalls": true
      },
      "SearchSuggestEnabled": false,
      "DisableTelemetry": true,
      "CaptivePortal": false,
      "Cookies": {
        "Default": true,
        "AcceptThirdParty": "never",
        "RejectTracker": true
      },
      "DNSOverHTTPS": {
        "Enabled": false
      },
      "UserMessaging": {
        "WhatsNew": false,
        "ExtensionRecommendations": false,
        "FeatureRecommendations": false,
        "UrlbarInterventions": false,
        "SkipOnboarding": true
      },
      "DisableFeedbackCommands": true,
      "DisableFirefoxStudies": true,
      "DisablePocket": true,
      "DontCheckDefaultBrowser": true,
      "EnableTrackingProtection": {
        "Value": true,
        "Cryptomining": true,
        "Fingerprinting": true
      },
      "Extensions": {
        "Install": [
{% for item in firefox_plugins %}
          "https://addons.mozilla.org/firefox/downloads/latest/{{ item }}/latest.xpi"{{ "," if not loop.last }}
{% endfor %}
        ]
      },
      "FirefoxHome": {
        "Search": true,
        "TopSites": true,
        "Highlights": false,
        "Pocket": false,
        "Snippets": false,
        "Locked": false
      },
      "HardwareAcceleration": true,
      "NetworkPrediction": false,
      "PopupBlocking": {
        "Allow": ["https://crypt.parrot.sh",
                  "https://pad.parrot.sh"],
        "Default": false,
        "Locked": false
      },
      "PromptForDownloadLocation": true,
      "SupportMenu": {
        "Title": "Parrot Support",
        "URL": "https://community.parrotsec.org",
        "AccessKey": "S"
      }
    }
  }